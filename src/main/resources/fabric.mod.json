{
  "schemaVersion": 1,
  "id": "chemlib",
  "version": "${version}",
  "name": "Chem Lib Fabric",
  "description": "Chem Lib ported to Fabric.",
  "authors": [
    "Dark_Arcana", "Madeline Sparkle"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://example.com/",
    "sources": "https://example.com/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/chemlib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smashingmods.chemlib.ChemLib"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.smashingmods.chemlib.ChemLibClient"
      }
    ]
  },
  "mixins": [
    "chemlib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.7.2+kotlin.1.6.20",
    "minecraft": "1.18.x",
    "java": ">=17"
  }
}
